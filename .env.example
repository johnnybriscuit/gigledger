# Tax Calculation Configuration
# These values are used for tax withholding estimates

# Current tax year
EXPO_PUBLIC_TAX_YEAR=2025

# Federal income tax flat rates (MVP - conservative estimates)
# TODO: Replace with bracket-based calculation
EXPO_PUBLIC_FEDERAL_FLAT_RATE_SINGLE=0.12
EXPO_PUBLIC_FEDERAL_FLAT_RATE_MARRIED=0.12
EXPO_PUBLIC_FEDERAL_FLAT_RATE_HOH=0.12

# Feature flags
EXPO_PUBLIC_USE_FEDERAL_BRACKETS=false

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Site URL (required for auth redirects)
EXPO_PUBLIC_SITE_URL=http://localhost:8090

# Google OAuth (optional, default: false)
# Set to 'true' to enable Google SSO button
# Requires Google OAuth client configured in Supabase
EXPO_PUBLIC_GOOGLE_OAUTH_ENABLED=false
